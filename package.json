{
  "name": "bggca",
  "license": "MIT",
  "repository": "https://github.com/BDQ/bgg_community_app",
  "devDependencies": {
    "async-wait-until": "^1.2.4",
    "babel-eslint": "^9.0.0",
    "babel-preset-expo": "^5.0.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "eslint": "^5.6.0",
    "eslint-plugin-jest": "^21.22.1",
    "eslint-plugin-react": "^7.11.1",
    "jest-expo": "^38.0.0",
    "react-native-dotenv": "^0.2.0"
  },
  "main": "node_modules/expo/AppEntry.js",
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "react",
      "jest"
    ],
    "env": {
      "es6": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:jest/recommended"
    ],
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
      "no-console": 0,
      "semi": [
        2,
        "never"
      ]
    },
    "globals": {
      "Headers": false,
      "fetch": false
    }
  },
  "scripts": {
    "start": "expo start",
    "debug": "open \"rndebugger://set-debugger-loc?host=localhost&port=19001\"",
    "test": "npx jest --coverage",
    "watch": "npx jest --watch",
    "buildTestEnv": "npx ./buildEnv.js > .env"
  },
  "jest": {
    "preset": "jest-expo",
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "./setupTests.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/?!(sentry)"
    ]
  },
  "dependencies": {
    "@code-bucket/board-game-geek": "^0.2.2",
    "@react-native-community/datetimepicker": "^3.4.3",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-cookies/cookies": "^6.0.7",
    "@react-navigation/bottom-tabs": "^5.1.1",
    "@react-navigation/material-top-tabs": "^5.3.14",
    "@react-navigation/native": "^5.0.9",
    "@react-navigation/stack": "^5.1.1",
    "debug": "^4.1.1",
    "expo": "^40.0.0",
    "expo-app-loading": "^1.0.3",
    "expo-cli": "^4.4.1",
    "expo-image-manipulator": "~7.0.0",
    "expo-image-picker": "~7.0.0",
    "himalaya": "^1.1.0",
    "next-frame": "^0.2.3",
    "prop-types": "^15.6.2",
    "react": "16.8.3",
    "react-async": "^10.0.0",
    "react-dom": "^16.6.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-40.0.0.tar.gz",
    "react-native-appearance": "~0.1.0",
    "react-native-datepicker": "^1.7.2",
    "react-native-dotenv": "^0.2.0",
    "react-native-elements": "^1.1.0",
    "react-native-flash-message": "^0.1.10",
    "react-native-gesture-handler": "~1.3.0",
    "react-native-html-parser": "^0.1.0",
    "react-native-htmlview": "^0.13.0",
    "react-native-image-progress": "^1.1.1",
    "react-native-image-zoom-viewer": "^2.2.26",
    "react-native-indicators": "^0.17.0",
    "react-native-maps": "^0.25.0",
    "react-native-material-dropdown-v2": "^0.11.1",
    "react-native-modal-datetime-picker": "^8.5.1",
    "react-native-paper": "^4.7.2",
    "react-native-progress": "^4.1.2",
    "react-native-reanimated": "~1.2.0",
    "react-native-render-html": "^5.1.0",
    "react-native-safe-area-context": "3.1.9",
    "react-native-safe-area-view": "^1.0.0",
    "react-native-screens": "~1.0.0-alpha.23",
    "react-native-searchable-dropdown": "^1.1.1",
    "react-native-simple-radio-button": "^2.7.4",
    "react-native-swipeout": "^2.3.6",
    "react-native-tab-view": "^2.16.0",
    "react-native-tag-select": "^2.0.0",
    "react-native-vector-icons": "^6.0.2",
    "react-native-xml2js": "^1.0.3",
    "react-xml-parser": "^1.1.6",
    "reactn": "^2.2.6",
    "sentry-expo": "~2.0.1",
    "throttle-debounce": "^1.0.1",
    "xml-js": "^1.6.11",
    "xpath": "0.0.32"
  }
}
